data/topology/*.txt
config/vmagent/targets/topology-switches.json
config/vmagent/targets/topology-servers.json

# 保留示例文件和目录结构
!data/topology/.gitkeep
!config/vmagent/targets/.gitkeep

# ===== 日志文件 =====
logs/
*.log
*.log.*
data/logs/
/var/log/network-devices/

